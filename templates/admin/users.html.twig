<table class="table table-striped">
    <thead>
        <tr>
        <th scope="col">ID</th>
        <th scope="col">Email</th>
        <th scope="col">Roles</th>
        </tr>
  </thead>
  <tbody>
  {% for user in users %}
    <tr>
      <th scope="row">{{user.id}}</th>
      <td>{{user.email}}</td>
      <td>{{user.roles|json_encode}}</td>
    </tr>
  {% endfor %}
  </tbody>
</table>